# Add
method add (x,y)
    return x+y

# Multiply
method multiply(a,b)
    return a*b

# Indirect
method callAdd(a,b)
    return add(a,4+5)

method PrecedenceCheck(ab,b,c,d)
    return ab+b*c+d

method divide(a,b)
    return a/b

# Main method
main
    4+5
    add(1,4)
    add(1.5,2.51)
    add(100,200)
    add(3,4)
    multiply(5, 8)
    multiply(5.73, 8.123)
    callAdd(10,3)
    PrecedenceCheck(1,3,6,7)
    divide(4,2)