# Add
method add (x,y)
    return x+y

# Multiply
method multiply(a,b)
    return a*b

# Indirect
method callAdd(a,b)
    return add(a,b)

method callcallAdd(a,b)
    return callAdd(a,b)

method PrecedenceCheck(ab,b,c,d)
    return ab+b*c+d

method divide(a,b)
    return a/b

method sub(a,b)
    return a-b

method callSub(a,b)
    return sub(b,a)

